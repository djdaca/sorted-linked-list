{
    "name": "djdaca/sorted-linked-list",
    "description": "Type-safe sorted doubly-linked list for PHP 8.4+",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "sorted",
        "linked-list",
        "data-structure",
        "type-safe",
        "php",
        "sorted-list"
    ],
    "autoload": {
        "psr-4": {
            "Djdaca\\SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Djdaca\\SortedLinkedList\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "DJDaca",
            "email": "djdaca@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "cs:fix": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-text",
        "stan": "vendor/bin/phpstan analyse src tests --level=9",
        "ci": [
            "@cs",
            "@stan",
            "@test"
        ]
    },
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.48"
    }
}
